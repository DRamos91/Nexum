package com.daniel.crud.model;

import javax.persistence.Entity;
import javax.persistence.Id;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Entity
@Data
@AllArgsConstructor
@NoArgsConstructor
public class Pessoa {
	
	@Id
	Long id;
	String nome;
	String email;
	String tipoPessoa;
	Long cpf;
	Long cnpj;
	Long cep;	
	String endereco;
	String logradouro;
	String bairro;
	String cidade;
	String estado;
	
	

}
