	CREATE TABLE CRUD (
	    ID BIGINT AUTO_INCREMENT NOT NULL,
	    NOME VARCHAR(50) NOT NULL,
	    EMAIL VARCHAR(100),
	    TIPO DE PESSOA VARCHAR(100),
	    CPF VARCHAR(100),
	    CNPJ VARCHAR(100),
	    CEP VARCHAR(100),
	    ENDERECO VARCHAR(100),
	    LOGRADOURO VARCHAR(100),
	    BAIRRO VARCHAR(100),
	    CIDADE VARCHAR(100),
	    ESTADO VARCHAR(100),
	    
	    SUMMARY TEXT,
	    CONSTRAINT PK_AUTHOR PRIMARY KEY (ID),
	    CONSTRAINT U_AUTHOR_NAME UNIQUE (NAME)
	);